{
  "name": "Branded Template",
  "description": "A template with branding elements",
  "format_type": "html",
  "content": "<!DOCTYPE html>\n<html>\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>{meeting_title}</title>\n    <style>\n        body {\n            font-family: {branding.font};\n            line-height: 1.6;\n            color: #333;\n            margin: 0;\n            padding: 0;\n            background-color: #f5f5f5;\n        }\n        \n        .container {\n            max-width: 800px;\n            margin: 0 auto;\n            padding: 20px;\n            background-color: #fff;\n            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n        }\n        \n        header {\n            border-bottom: 2px solid {branding.secondary_color};\n            padding-bottom: 20px;\n            margin-bottom: 20px;\n        }\n        \n        .logo {\n            max-width: 150px;\n            margin-bottom: 15px;\n        }\n        \n        h1 {\n            color: {branding.secondary_color};\n            margin: 0 0 10px 0;\n        }\n        \n        h2 {\n            color: {branding.primary_color};\n            margin: 0 0 10px 0;\n        }\n        \n        h3 {\n            color: {branding.secondary_color};\n            border-bottom: 1px solid #eee;\n            padding-bottom: 5px;\n        }\n        \n        .date-time {\n            color: #7f8c8d;\n            font-style: italic;\n        }\n        \n        section {\n            margin-bottom: 20px;\n        }\n        \n        .content {\n            padding-left: 15px;\n        }\n        \n        footer {\n            margin-top: 30px;\n            padding-top: 10px;\n            border-top: 1px solid {branding.secondary_color};\n            color: #7f8c8d;\n            font-size: 0.8em;\n            text-align: center;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"container\">\n        <header>\n            <img class=\"logo\" src=\"{branding.logo}\" alt=\"Logo\">\n            <h1>{branding.header}</h1>\n            <h2>{meeting_title}</h2>\n            <p class=\"date-time\">{date_time}</p>\n        </header>\n        \n        <section>\n            <h3>Attendees</h3>\n            <div class=\"content\">\n                {attendees}\n            </div>\n        </section>\n        \n        <section>\n            <h3>Agenda</h3>\n            <div class=\"content\">\n                {agenda}\n            </div>\n        </section>\n        \n        <section>\n            <h3>Key Discussion Points</h3>\n            <div class=\"content\">\n                {discussion_points}\n            </div>\n        </section>\n        \n        <section>\n            <h3>Action Items</h3>\n            <div class=\"content\">\n                {action_items}\n            </div>\n        </section>\n        \n        <section>\n            <h3>Decisions Made</h3>\n            <div class=\"content\">\n                {decisions}\n            </div>\n        </section>\n        \n        <section>\n            <h3>Next Steps</h3>\n            <div class=\"content\">\n                {next_steps}\n            </div>\n        </section>\n        \n        <footer>\n            <p>{branding.footer}</p>\n        </footer>\n    </div>\n</body>\n</html>",
